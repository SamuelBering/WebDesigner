@using WebDesigner.Models.ViewModels
@using EPiServer.Web.Mvc.Html
@model PageViewModel<WebDesigner.Models.Pages.StaffPage>

@{
    Layout = "~/Views/Shared/Layouts/_LeftNavigation.cshtml";
}

<div class="aboutus">

    <div class="container">
        <div class="col-md-12 wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="300ms">
            <img src="@(Model.CurrentPage.StaffImage!=null ? Url.ContentUrl(Model.CurrentPage.StaffImage):Url.Content("~/static/images/services/1.jpg"))" alt="">
            @Html.PropertyFor(m => m.CurrentPage.MainBody)

        </div>
    </div>
</div>